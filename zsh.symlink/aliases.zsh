
## Aliases

alias vim='nvim'
alias zshrc="vim ~/.zshrc"
alias xcode="open -a Xcode"
alias exit="exit 0"
alias yif="yarn install --force"
alias rnm="rm -rf ./node_modules && yif"
alias inc=". ~/.localrc"
alias acorn="vim ~/Documents/Indigo/acorn"
alias dev="vim ~/Documents/DEV"
alias im="vim ~/Documents/Q"
alias indigo="vim ~/Documents/Indigo"
alias dotf="vim ~/Documents/DEV/dotfiles"
alias mapk="vim ~/Documents/DEV/dotfiles/vim.symlink/config/mappings.vim"
